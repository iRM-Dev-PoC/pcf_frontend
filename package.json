    "type": "module",
    "scripts": {
        "dev": "vite --port 5001 --strictPort --host",
        "build": "tsc && vite build",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "preview": "vite preview --port 5001 --strictPort --host",
