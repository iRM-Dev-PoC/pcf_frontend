        "docker:run": "docker run -p 5001:5001 trppritysingh/pcf_frontend:latest",
        "docker:push": "docker build -t trppritysingh/pcf_frontend:latest . && docker push trppritysingh/pcf_frontend:latest",
        "cf:push": "docker build -t trppritysingh/pcf_frontend:latest . && docker push trppritysingh/pcf_frontend:latest && cf push -f manifest.yml"
